<Application x:Class="FWS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary >
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="style/MainWindowStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="style/CitySelectStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ControlTemplate/MainWindowTemplate.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ControlTemplate/CitySelectTemplate.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
